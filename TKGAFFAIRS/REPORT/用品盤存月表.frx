      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="SELECT 品號,品名,規格,單位,期初數量,期初金額,本期進貨數量,本期進貨金額,本期領料數量,本期領料金額,(期初數量+本期進貨數量-本期領料數量) AS 期末數量,(期初金額+本期進貨金額-本期領料金額) AS 期末金額&#13;&#10;FROM (&#13;&#10;SELECT [MB001] AS '品號',[MB002] AS '品名',[MB003] AS '規格',[UNIT] AS '單位'&#13;&#10;,(SELECT ISNULL(SUM(NUM),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001] AND DATES&lt;='2018/11/30') AS '期初數量'&#13;&#10;,(SELECT ISNULL(SUM(TOTALMONEY),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001] AND DATES&lt;='2018/11/30') AS '期初金額'&#13;&#10;,(SELECT ISNULL(SUM(NUM),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001] AND KINID='採購' AND DATES&gt;='2018/12/1' AND DATES&lt;='2018/12/31') AS '本期進貨數量'&#13;&#10;,(SELECT ISNULL(SUM(TOTALMONEY),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001]  AND KINID='採購'  AND DATES&gt;='2018/12/1' AND DATES&lt;='2018/12/31') AS '本期進貨金額'&#13;&#10;,(SELECT ISNULL(SUM(NUM),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001] AND KINID='領用' AND DATES&gt;='2018/12/1' AND DATES&lt;='2018/12/31') AS '本期領料數量'&#13;&#10;,(SELECT ISNULL(SUM(TOTALMONEY),0) FROM  [TKGAFFAIRS].[dbo].[INVGAFFAIRS] WHERE [INVGAFFAIRS].MB001=[INVMB].[MB001]  AND KINID='領用'  AND DATES&gt;='2018/12/1' AND DATES&lt;='2018/12/31') AS '本期領料金額'&#13;&#10;FROM [TKGAFFAIRS].[dbo].[INVMB]&#13;&#10;) AS TEMP&#13;&#10;ORDER BY 品號">
